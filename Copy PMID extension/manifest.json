{
    "name": "CopyPMID",
    "version": "1.2",
    "manifest_version": 2,
    "description": "Adds a \"Copy PMID\" button to NLM PubMed pages.",
    "author": "T W Lee",
    "content_scripts": [
        {
            "matches": ["http://*.nlm.nih.gov/pubmed*"],
            "js": ["pmid_inject.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "clipboardWrite",
        "clipboardRead"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }

}
